{
  "name": "exifr",
  "version": "3.0.0",
  "description": "ðŸ“· The fastest and most versatile JavaScript EXIF reading library.",
  "author": "Mike Kovarik",
  "license": "MIT",
  "keywords": [
    "exif",
    "jpg",
    "jpeg",
    "heic",
    "tiff",
    "xmp",
    "gps",
    "icc",
    "iptc",
    "jfif",
    "image",
    "picture",
    "photo",
    "metadata",
    "makernote"
  ],
  "homepage": "https://mutiny.cz/exifr/",
  "bugs": "https://github.com/MikeKovarik/exifr/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/MikeKovarik/exifr.git"
  },
  "main": "index.cjs",
  "module": "index.mjs",
  "type": "module",
  "types": "index.d.ts",
  "scripts": {
    "test": "mocha test/**.spec.js --exclude test/bundle*.spec.js",
    "test-bundle": "mocha ./test/bundle-core.spec.js && mocha ./test/bundle-lite.spec.js && mocha ./test/bundle-full.spec.js",
    "coverage": "c8 npm test",
    "build": "rollup -c rollup.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "aurelia-script": "^1.5.2",
    "babel-preset-minify": "^0.5.1",
    "c8": "^7.0.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "mocha": "7.0.0-esm1",
    "rollup": "^1.25.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-notify": "^1.0.1"
  },
  "c8": {
    "exclude": [
      "src/file-readers/BlobReader.js",
      "src/file-readers/UrlFetcher.js",
      "src/util/debug.js",
      "test/**.*"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary"
    ]
  },
  "dependencies": {}
}
