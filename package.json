{
  "name": "exifr",
  "version": "2.1.3",
  "description": "ðŸ“· The fastest and most versatile JavaScript EXIF reading library.",
  "author": "Mike Kovarik",
  "license": "MIT",
  "keywords": [
    "exif",
    "tiff",
    "jpg",
    "jpeg",
    "xmp",
    "gps",
    "jfif",
    "icc",
    "iptc",
    "image",
    "picture",
    "photo",
    "buffer",
    "uint8array",
    "metadata",
    "makernote"
  ],
  "homepage": "https://mutiny.cz/exifr/",
  "bugs": "https://github.com/MikeKovarik/exifr/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/MikeKovarik/exifr.git"
  },
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "mocha -r esm ./test/**.spec.js --watch",
    "test": "mocha -r esm ./test/**.spec.js",
    "test-jfif": "npm test -- --grep=JFIF",
    "test-tiff": "npm test -- --grep=TIFF",
    "test-iptc": "npm test -- --grep=IPTC",
    "test-icc": "npm test -- --grep=ICC",
    "test-xmp": "npm test -- --grep=XMP",
    "coverage": "nyc npm test",
    "build": "rollup -c rollup.config.js"
  },
  "devDependencies": {
    "aurelia-script": "^1.5.2",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "esm": "^3.2.25",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "rollup": "^1.25.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-notify": "^1.0.1"
  },
  "nyc": {
    "exclude": [
      "src/index-full.js",
      "src/index-lite.js",
      "src/options.js",
      "src/util/debug.js",
      "src/parsers/tiff-tags.js",
      "src/tags/**.*",
      "test/**.*"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary"
    ],
    "require": ["esm"],
    "check-coverage": false,
    "cache": true
  },
  "esm": {
    "cjs": true
  }
}
